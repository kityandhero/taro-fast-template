{
  "name": "taro-fast-template",
  "version": "1.0.0",
  "private": true,
  "description": "taro-fast-template",
  "templateInfo": {
    "name": "default",
    "typescript": false,
    "css": "less"
  },
  "scripts": {
    "prepare": "husky install",
    "start": "npm run dev:weapp",
    "start:h5": "npm run dev:h5",
    "build:weapp": "npx taro build --type weapp",
    "build:swan": "npx taro build --type swan",
    "build:alipay": "npx taro build --type alipay",
    "build:tt": "npx taro build --type tt",
    "build:h5": "npx taro build --type h5",
    "build:rn": "npx taro build --type rn",
    "build:qq": "npx taro build --type qq",
    "build:jd": "npx taro build --type jd",
    "build:quickapp": "npx taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint:prettier": "prettier --check \"**/*\" --end-of-line auto",
    "precommit": "lint-staged",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:style": "stylelint --fix \"./src/**/*.less\"",
    "prettier": "prettier -c --write \"**/*\"",
    "cz": "cz",
    "scz": "git stage -A && npm run cz && git push",
    "commitlint": "npx commitlint --edit",
    "clean": "rimraf ./yarn-error.log && rimraf ./yarn.lock && rimraf ./package-lock.json && rimraf ./node_modules",
    "reinstall": "npm run clean && git pull && taro-fast-cli env --agent http://127.0.0.1:7890 && pnpm install"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "dependencies": {
    "@tarojs/components": "3.4.9",
    "@tarojs/react": "3.4.9",
    "@tarojs/runtime": "3.4.9",
    "@tarojs/taro": "3.4.9",
    "dayjs": "^1.11.2",
    "dva-core": "^2.0.4",
    "dva-loading": "^3.0.22",
    "postcss-less": "^6.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.8",
    "redux": "^4.2.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.1",
    "taro-fast-common": "^1.0.55",
    "taro-fast-component": "^2.0.92",
    "taro-fast-component-extra": "^2.0.90",
    "taro-fast-framework": "^1.0.128"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-external-helpers": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.9",
    "@babel/preset-react": "^7.16.7",
    "@babel/runtime": "^7.17.9",
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@commitlint/cz-commitlint": "^16.2.4",
    "@prefresh/babel-plugin": "^0.4.3",
    "@tarojs/mini-runner": "3.4.9",
    "@tarojs/plugin-framework-react": "^3.4.9",
    "@tarojs/webpack-runner": "3.4.9",
    "@types/react": "^18.0.9",
    "@types/webpack-env": "^1.16.4",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-import": "^1.13.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-taro": "3.4.9",
    "classnames": "^2.3.1",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-taro": "3.4.9",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "husky": "^8.0.1",
    "inquirer": "^8.2.4",
    "lint-staged": "^12.4.1",
    "postcss": "^8.4.13",
    "postcss-pxtorem": "^6.0.0",
    "preact": "^10.7.2",
    "prettier": "^2.6.2",
    "react-refresh": "^0.13.0",
    "stylelint": "^14.8.2",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "typescript": "^4.6.4"
  },
  "optionalDependencies": {
    "taro-fast-cli": "^1.0.4"
  }
}
